msgid "under Windows:"
msgstr "Unter Windows:"

#. type: Plain text
#: eeschema_creating_customized_netlists_and_bom_files.adoc:867
#, no-wrap
msgid "*f:/kicad/bin/xsltproc.exe -o \"%O\" f:/kicad/bin/plugins/netlist_form_pads-pcb.xsl \"%I\"*\n"
