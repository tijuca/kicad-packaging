msgid "under Linux:"
msgstr "Unter Linux:"

#. type: Plain text
#: eeschema_creating_customized_netlists_and_bom_files.adoc:871
#, no-wrap
msgid "*xsltproc -o \"%O\" /usr/local/kicad/bin/plugins/netlist_form_pads-pcb.xsl \"%I\"*\n"
msgstr ""

#. type: Plain text
#: eeschema_creating_customized_netlists_and_bom_files.adoc:874
