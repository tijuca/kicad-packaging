#: kicad.adoc:466
#, no-wrap
msgid "images/template_tree_meta.png"
msgstr ""

#. type: Plain text
#: kicad.adoc:469
msgid "brd.png is an optional file."
msgstr "brd.png adalah file tambahan yang bersifat opsional."

