#, no-wrap
msgid "gerbview_layer_select_1_png"
msgstr "gerbview_layer_select_1_png"

#. type: Target for macro image
#: gerbview.adoc:237
#, no-wrap
msgid "images/gerbview_layer_select_1.png"
msgstr "images/gerbview_layer_select_1.png"

#. type: Plain text
