"            </xsl:when>\n"
"            <xsl:otherwise>\n"
"                <xsl:text>N-</xsl:text>\n"
"                <xsl:value-of select=\"@code\"/>\n"
"            </xsl:otherwise>\n"
"        </xsl:choose>\n"
"        <xsl:text>&nl;</xsl:text>\n"
"        <xsl:apply-templates select=\"node\"/>\n"
"    </xsl:if>\n"
"</xsl:template>\n"
msgstr ""
