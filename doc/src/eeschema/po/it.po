"    <xsl:value-of select=\"@ref\"/>\n"
"    <xsl:text> </xsl:text>\n"
"    <xsl:choose>\n"
"        <xsl:when test = \"footprint != '' \">\n"
"            <xsl:apply-templates select=\"footprint\"/>\n"
"        </xsl:when>\n"
"        <xsl:otherwise>\n"
"            <xsl:text>unknown</xsl:text>\n"
"        </xsl:otherwise>\n"
"    </xsl:choose>\n"
"    <xsl:text>&nl;</xsl:text>\n"
"</xsl:template>\n"
msgstr ""
