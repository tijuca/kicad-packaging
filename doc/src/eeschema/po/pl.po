#: eeschema_main_top_menu.adoc:85
#, no-wrap
msgid "Preferences menu / Schematic Editor Options"
msgstr "Ustawienia / Opcje edytora schematów"

#. type: Named 'alt' AttributeList argument for macro 'image'
#: eeschema_main_top_menu.adoc:87 eeschema_general_top_toolbar.adoc:25
#, no-wrap
msgid "Schematic Editor Options"
msgstr "Opcje edytora schematów"

