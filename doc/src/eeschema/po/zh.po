#, no-wrap
msgid ""
"<xsl:stylesheet version=\"1.0\" xmlns:xsl=\"http://www.w3.org/1999/XSL/Transform\">\n"
"<xsl:output method=\"text\" omit-xml-declaration=\"yes\" indent=\"no\"/>\n"
msgstr ""
