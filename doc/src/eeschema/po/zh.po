#, no-wrap
msgid ""
"<!-- for each node -->\n"
"<xsl:template match=\"node\">\n"
"    <xsl:text> </xsl:text>\n"
"    <xsl:value-of select=\"@ref\"/>\n"
"    <xsl:text>.</xsl:text>\n"
"    <xsl:value-of select=\"@pin\"/>\n"
