#: pcbnew_general_operations.adoc:219
#, no-wrap
msgid "images/Pcbnew_top_menu_bar.png"
msgstr "images/ru/Pcbnew_top_menu_bar.png"

#. type: Title ====
#: pcbnew_general_operations.adoc:221
#, no-wrap
msgid "The File menu"
msgstr "Меню Файл"

#. type: Target for macro image
