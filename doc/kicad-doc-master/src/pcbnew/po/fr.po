msgstr ""

#. type: Plain text
#: Pcbnew_general_operations.adoc:13
msgid "right toolbar menu."
msgstr ""

#. type: Plain text
#: Pcbnew_general_operations.adoc:15
msgid "left toolbar menu."
msgstr ""

#. type: Plain text
#: Pcbnew_general_operations.adoc:17
msgid "mouse buttons (menu options). Specifically:"
