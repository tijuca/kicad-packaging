msgstr "100000000000016D000000E7007BA5B8_png"

#. type: Target for macro image
#: eeschema_viewlib.adoc:13
#, no-wrap
msgid "images/100000000000016D000000E7007BA5B8.png"
msgstr "images/100000000000016D000000E7007BA5B8.png"

#. type: Title ===
#: eeschema_viewlib.adoc:16
#, no-wrap
msgid "Viewlib - main screen"
msgstr "Viewlib - pantalla principal"

#. type: Named 'alt' AttributeList argument for macro 'image'
