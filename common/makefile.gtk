## Makefile for common.a
CC = gcc

# Compiler flags.
CPPFLAGS =  -Wall -O2 -I./ -I../include `wx-config --cflags`

EDACPPFLAGS = $(CPPFLAGS)

all: common.a

include ../libs.linux

include makefile.include

CPPFLAGS += $(EXTRACPPFLAGS) -fno-strict-aliasing
EDACPPFLAGS = $(CPPFLAGS)


common.a: $(OBJECTS) makefile.gtk makefile.include
	rm -f $@
	ar -rv $@ $(OBJECTS)
	ranlib $@

install:common.a

clean:
	rm -f *.o; rm -f *~; rm core; rm *.bak; rm *.obj
	rm -f common.a


